Programa -> Bloque Programa | Declaracion_funcion Programa | e
Declaracion_funcion -> "proc" Tipo_dato Id "(" Parametros_funcion ")" Bloque
Parametros_funcion -> Declaracion Parametros_funcion_aux | e
Parametros_funcion_aux -> "," Declaracion Parametros_funcion_aux | e
Retorno_funcion -> "return" Expresion ";"
Bloque -> "start" BloqueAux "end"
BloqueAux -> Instruccion BloqueAux | e
Instruccion -> Asignacion ";" | Declaracion ";" | For | While | If | Exec_funcion ";" | Retorno_funcion ";"
Asignacion -> Id "=" Expresion | Id "=" "[" Lista_valores "]"
Lista_valores -> Expresion Lista_valores_aux | e
Lista_valores_aux -> "," Expresion Lista_valores_aux | e
Declaracion -> Tipo_dato Id | Tipo_dato Id "=" Expresion | Tipo_dato Id "[" Numero_int "]" | Tipo_dato Id "[" Numero_int "]" "=" "[" Lista_valores "]"
For -> for ( Declaracion ; Expresion_booleana; Asignacion;) Bloque
While -> while ( Expresion_booleana ) Bloque
If -> Matched | Unmatched
Matched ->"if" Expresion Matched "else" Matched
            | "if" Expresion Bloque  "else" Matched
            | "if" Expresion Matched "else" Bloque
            | "if" Expresion Bloque  "else" Bloque
            | Other
            | Bloque
Unmatched -> "if" Expr Bloque
            | "if" Expr Matched "else" Unmatched
Exec_funcion -> exec Id ( Argumentos )
Argumentos -> Id ArgumentosAux | e
ArgumentosAux -> , Id ArgumentosAux | e
Expresion -> Expresion_booleana | Expresion_aritmetica
Expresion_aritmetica -> Termino TerminoAux
Termino -> Factor FactorAux
TerminoAux -> "+" Termino TerminoAux | "-" Termino TerminoAux | e
Factor -> Id | Valor | ( Expresion_aritmetica ) | Id [ Expresion_aritmetica ]
FactorAux -> "*" Factor FactorAux | "/" Factor FactorAux | "%" Factor FactorAux |  e
Expresion_booleana -> Expresion_and Expresion_booleanaAux
Expresion_booleanaAux -> "||" Expresion_and Expresion_booleanaAux | e
Expresion_and -> Expresion_not Expresion_andAux
Expresion_andAux -> "&&" Expresion_not Expresion_andAux | e
Expresion_not -> "!" Expresion_relacional | Expresion_relacional
Expresion_relacional -> Id [ Expresion_aritmetica ] | Id | Expresion_aritmetica Op_relacional Expresion_aritmetica | ( Expresion_booleana ) | true | false
Op_relacional -> "==" | "!=" | "<" | ">" | "<=" | ">="
Tipo_dato -> "int" | "bool" | "char" | "float"
Valor -> Numero_int | Numero_float | Caracter | Booleano
Booleano -> "true" | "false"
Numero_int -> [0-9]+
Numero_float -> [0-9]+"."[0-9]+
Caracter -> "'" . "'"
Id -> [a-zA-Z_][a-zA-Z0-9_]*