void proc loop() start
  int izquierda = exec readLeftSensor();
  int derecha = exec readRightSensor();
  
  if (izquierda == 1 and derecha == 1) start
    exec forward_ms();
  end
  else
  start
    if (izquierda == 0 and derecha == 1) start
      exec turnRight_ms();
    end
    else
    start
      if (izquierda == 1 and derecha == 0) start
        exec forward_ms();
      end
    end
  end
end
start
    exec stopMotors();
end
