void proc loop() start
  int izquierda = exec readLeftSensor();
  int derecha = exec readRightSensor();
  exec print(izquierda);
  exec print(derecha);
  if (izquierda == 0 and derecha == 0) start
    exec forward_ms(0);
  end
  else
  start
    if (izquierda == 0 and derecha == 1) start
      exec turnRight_ms(0);
    end
    else
    start
      if (izquierda == 1 and derecha == 0) start
        exec turnLeft_ms(0);
      end
    end
  end
end
start
  exec print(51);
end
